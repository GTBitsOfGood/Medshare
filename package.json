{
  "name": "medshare",
  "version": "1.0.0",
  "description": "Inventory management solution for Medshare",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"**/*.js\"",
    "format": "prettier --write \"**/*.{js,css}\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,jsx}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GTBitsOfGood/medshare.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GTBitsOfGood/medshare/issues"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "react": "^16.12.0",
    "@blueprintjs/core": "^3.23.1",
    "@blueprintjs/select": "^3.11.2",
    "@okta/okta-auth-js": "^3.0.1",
    "@okta/okta-react": "^3.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "prop-types": "^15.7.2",
    "react-dom": "^16.12.0",
    "react-file-drop": "^3.0.6",
    "react-infinite-scroll-component": "^5.0.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "styled-components": "^5.0.1",
    "unstated-next": "^1.1.0"
  }
}
